{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Aeternum Map-DEV",
    "author": "Leon Machens",
    "version": "2.21.4",
    "minimum-overwolf-version": "0.169.0",
    "description": "New World Interactive Map",
    "dock_button_title": "Aeternum Map",
    "icon": "icons/iconMouseOver.png",
    "icon_gray": "icons/iconMouseNormal.png",
    "launcher_icon": "icons/desktopIcon.ico",
    "window_icon": "icons/taskbarIcon.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "Media",
    "FileSystem",
    "DesktopStreaming",
    "Clipboard"
  ],
  "data": {
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "debug_url": "http://localhost:5173/background.html",
        "is_background_page": true,
        "allow_local_file_access": true
      },
      "desktop": {
        "file": "sender.html",
        "debug_url": "http://localhost:5173/sender.html",
        "desktop_only": true,
        "native_window": true,
        "resizable": false,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 400,
          "height": 660
        },
        "min_size": {
          "width": 400,
          "height": 660
        },
        "max_size": {
          "width": 400,
          "height": 660
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "keep_window_location": true,
        "disable_hardware_acceleration": true
      },
      "minimap": {
        "file": "minimap.html",
        "debug_url": "http://localhost:5173/minimap.html",
        "desktop_only": false,
        "in_game_only": true,
        "style": "inputPassThrough",
        "resizable": false,
        "transparent": true,
        "override_on_update": false,
        "size": {
          "width": 260,
          "height": 260
        },
        "start_position": {
          "top": 50,
          "left": 50
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "keep_window_location": true,
        "restrict_to_game_bounds": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [21816]
    },
    "game_events": [21816],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [21816]
        },
        "start_minimized": false
      }
    ],
    "hotkeys": {
      "show_hide_app": {
        "title": "Show/Hide App",
        "action-type": "custom",
        "default": "F4"
      },
      "show_hide_minimap": {
        "title": "Show/Hide Minimap",
        "action-type": "custom",
        "default": "F5"
      },
      "setup_minimap": {
        "title": "Setup Minimap",
        "action-type": "custom",
        "default": "Ctrl+F5"
      },
      "zoom_in_minimap": {
        "title": "Zoom In Minimap",
        "action-type": "custom",
        "default": "F6"
      },
      "zoom_out_minimap": {
        "title": "Zoom Out Minimap",
        "action-type": "custom",
        "default": "F7"
      },
      "zoom_in_map": {
        "title": "Zoom In Map (Website)",
        "action-type": "custom",
        "default": "F8"
      },
      "zoom_out_map": {
        "title": "Zoom Out Map (Website)",
        "action-type": "custom",
        "default": "F9"
      },
      "marker_action": {
        "title": "Interact with near marker (Website)",
        "action-type": "custom",
        "default": "E",
        "passthrough": true
      },
      "show_hide_direction": {
        "title": "Show/Hide Direction (Website)",
        "action-type": "custom",
        "default": "F11"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://localhost:6006",
        "http://localhost:5173",
        "https://aeternum-map.gg"
      ]
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
